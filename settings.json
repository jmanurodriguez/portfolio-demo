{
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "postcss.validate": true,
    "files.associations": {
      "*.css": "postcss"
    },
    "editor.quickSuggestions": {
      "strings": true
    },
    "tailwindCSS.includeLanguages": {
      "html": "html",
      "javascript": "javascript",
      "css": "postcss"
    },
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    },
    "files.watcherExclude": {
      "**/node_modules/**": true
    }
  }